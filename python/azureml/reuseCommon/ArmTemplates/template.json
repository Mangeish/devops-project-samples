    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "type": "string"
        },
        "location": {
            "type": "string"
        }
    },
    "resources": [
        {
            "apiVersion": "2018-11-19",
            "name": "[parameters('name')]",
            "location": "[parameters('location')]",
